<?xml version="1.0"?>
<extension key="com.proveg.api" type="module">
  <file>provegapi</file>
  <name>ProVeg API</name>
  <description>Provides customized interfaces for applications to communicate with CiviCRM.</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Jens Schuppe</author>
    <email>schuppe@systopia.de</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/systopia/com.proveg.api</url>
    <url desc="Documentation">https://github.com/systopia/com.proveg.api/blob/master/README.md</url>
    <url desc="Support">https://github.com/systopia/com.proveg.api/issues</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2020-08-07</releaseDate>
  <version>0.6</version>
  <develStage>dev</develStage>
  <compatibility>
    <ver>5.27</ver>
  </compatibility>
  <comments/>
  <requires>
    <ext>de.systopia.xcm</ext>
  </requires>
  <civix>
    <namespace>CRM/ProvegAPI</namespace>
    <format>23.02.1</format>
  </civix>
  <mixins>
    <mixin>menu-xml@1.0.0</mixin>
    <mixin>smarty-v2@1.0.1</mixin>
  </mixins>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
</extension>
